<!DOCTYPE html>
<html>
  <head>
    <title>NbaProject</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if user_signed_in? %>
    <p>Hello <%= current_user .uname%> - <%= link_to "sign_out",destroy_user_session_path, :method=> 'delete'%></p>
    <% end%>
    <%= yield %>
  </body>
</html>
